{
    "name": "codeblastr/users",
    "description": "User management plugin for CakePHP 3.x, which extends the CakeDC users plugin.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "users",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Kersey",
            "homepage": "http://www.richardkersey.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/codeblastr/users/graphs/contributors"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.2",
        "friendsofcake/crud": "~4.2",
        "admad/cakephp-jwt-auth": "^2.0",
        "cakedc/users": "*",

        "phpunit/phpunit": "~4.1",

        "codeblastr/queue": "*"
    },
    "autoload": {
        "psr-4": {
            "CodeBlastrUsers\\": "src",
            "CodeBlastrUsers\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeBlastrUsers\\Users\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
